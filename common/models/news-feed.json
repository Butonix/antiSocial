{
  "name": "NewsFeed",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "strictObjectIDCoercion": true,
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "uuid": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "about": {
      "type": "string"
    },
    "details": {
      "type": "object"
    },
    "humanReadable": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "friend": {
      "type": "belongsTo",
      "model": "Friend",
      "foreignKey": "friendId"
    }
  },
  "acls": [],
  "methods": {}
}
