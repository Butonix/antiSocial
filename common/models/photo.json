{
  "name": "Photo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "strictObjectIDCoercion": true,
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "status": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "geoDescription": {
      "type": "string"
    },
    "geoLocation": {
      "type": [
        "number"
      ]
    },
    "uuid": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "photoId",
      "through": "PostPhoto"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "polymorphic": {
        "foreignKey": "foreignId",
        "discriminator": "foreignType"
      }
    },
    "reactions": {
      "type": "hasMany",
      "model": "Reaction",
      "polymorphic": {
        "foreignKey": "foreignId",
        "discriminator": "foreignType"
      }
    }
  },
  "acls": [],
  "methods": {}
}
