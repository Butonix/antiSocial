extends ../wrapper

block seo
	title.
		Reanimate

block content
	.container-fluid.content-padding-top.content

		.page-header
			h1.
				Confirm Import user data from .zip file
	
		table.table
			tr
				td User Name
				td= theUser.username
			tr
				td Friends
				td= theUser.friends.length
			tr
				td Posts
				td= theUser.posts.length
		
		form(method="POST" action="/reanimate")
			input(type="hidden" name="archive" value= archive)
			.form-group
				button.btn.btn.primary Do It!
